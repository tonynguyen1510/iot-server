{
	"name": "TicketSelling",
	"plural": "ticket-sellings",
	"base": "PersistedModel",
	"idInjection": true,
	"options": {
		"validateUpsert": true
	},
	"scope": {
		"order": "createdAt DESC"
	},
	"properties": {
		"createdAt": {
			"type": "date",
			"defaultFn": "now"
		},
		"updatedAt": {
			"type": "date",
			"defaultFn": "now"
		},
		"images": {
			"type": [
				"string"
			]
		},
		"content": {
			"type": "string"
		},
		"dataType": {
			"type": "string",
			"default": "system"
		},
		"airline": {
			"type": "string",
			"default": "all"
		},
		"fbData": {
			"type": "object"
		},
		"flightType": {
			"type": "string",
			"required": true,
			"default": "oneWay"
		},
		"seatType": {
			"type": "string"
		},
		"packageWeight": {
			"type": "string"
		},
		"seatCount": {
			"type": "number"
		},
		"isBid": {
			"type": "boolean",
			"default": false
		},
		"bidDueDate": {
			"type": "date"
		},
		"startingPrice": {
			"type": "number"
		},
		"price": {
			"type": "number"
		},
		"status": {
			"type": "string",
			"default": "open"
		},
		"trip": {
			"type": {
				"departure": {
					"type": "string"
				},
				"destination": {
					"type": "string"
				},
				"startDate": {
					"type": "date",
					"defaultFn": "now"
				},
				"startTime": {
					"type": "string"
				}
			}
		},
		"tripBack": {
			"type": {
				"departure": {
					"type": "string"
				},
				"destination": {
					"type": "string"
				},
				"startDate": {
					"type": "date",
					"defaultFn": "now"
				},
				"startTime": {
					"type": "string"
				}
			}
		}
	},
	"validations": [],
	"relations": {
		"bids": {
			"type": "hasMany",
			"model": "SellBid",
			"foreignKey": "ticketBuyId",
			"primaryKey": "id"
		},
		"currentBid": {
			"type": "belongsTo",
			"model": "SellBid",
			"foreignKey": "currentBidId",
			"primaryKey": "id"
		},
		"seller": {
			"type": "belongsTo",
			"model": "user",
			"foreignKey": "sellerId",
			"required": true
		},
		"buyer": {
			"type": "belongsTo",
			"model": "user",
			"foreignKey": "buyerId",
			"required": true
		}
	},
	"acls": [],
	"methods": {}
}
