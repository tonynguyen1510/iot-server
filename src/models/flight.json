{
	"name": "Flight",
	"plural": "flights",
	"base": "PersistedModel",
	"idInjection": true,
	"options": {
		"validateUpsert": true
	},
	"scope": {
		"order": "createdAt DESC"
	},
	"properties": {
		"createdAt": {
			"type": "date",
			"defaultFn": "now"
		},
		"updatedAt": {
			"type": "date",
			"defaultFn": "now"
		},
		"content": {
			"type": "string"
		},
		"from": {
			"type": "object"
		},
		"feedFBId": {
			"type": "string"
		},
		"departure": {
			"type": "string"
		},
		"destination": {
			"type": "string"
		},
		"status": {
			"type": "string",
			"required": true
		},
		"startDate": {
			"type": "date",
			"defaultFn": "now"
		},
		"endDate": {
			"type": "date"
		},
		"bidDueDate": {
			"type": "date"
		},
		"flightType": {
			"type": "string",
			"required": true,
			"default": "One way"
		},
		"type": {
			"type": "string",
			"required": true,
			"default": "Sell"
		},
		"seatType": {
			"type": "string"
		},
		"packageWeight": {
			"type": "string"
		},
		"stock": {
			"type": "number"
		},
		"startingPrice": {
			"type": "number"
		},
		"price": {
			"type": "number"
		}
	},
	"validations": [],
	"relations": {
		"bids": {
			"type": "hasMany",
			"model": "Bid",
			"foreignKey": "flightId",
			"primaryKey": "id"
		},
		"currentBid": {
			"type": "belongsTo",
			"model": "Bid",
			"foreignKey": "currentBidId",
			"primaryKey": "id"
		},
		"seller": {
			"type": "belongsTo",
			"model": "user",
			"foreignKey": "sellerId",
			"required": true
		},
		"buyer": {
			"type": "belongsTo",
			"model": "user",
			"foreignKey": "buyerId",
			"required": true
		}
	},
	"acls": [],
	"methods": {}
}
