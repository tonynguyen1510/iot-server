		},
		"author": {
			"type": "object",
			"require": true
		},
		"content": {
			"type": "string",
			"require": true
		},
		"status": {
			"type": "string",
			"default": "pending"
		}
	},
	"validations": [],
	"relations": {
	},
	"acls": [],
	"methods": {}
