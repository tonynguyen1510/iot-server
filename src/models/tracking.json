{
	"name": "Tracking",
	"base": "PersistedModel",
	"idInjection": true,
	"options": {
	"validateUpsert": true
	},
	"scope": {
		"order": "createdAt DESC"
	},
	"properties": {
		"createdAt": {
			"type": "date",
			"defaultFn": "now"
		},
		"status": {
			"type": "string",
			"required": true
		},
		"type": {
			"type": "string",
			"required": true
		},
		"ticket": {
			"type": "object",
			"required": true
		}
	},
	"validations": [],
	"relations": {
		"author": {
			"type": "belongsTo",
			"model": "user",
			"foreignKey": "userId",
			"required": true
		}
	},
	"acls": [],
	"methods": {}
}
